{
  "name": "ng-practice-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "___ Manual Lint ___": "",
    "man:space:format": "npm run ci:space:format -- --write",
    "man:space:styles": "npm run ci:space:styles -- --fix",
    "man:space:lint": "npm run ci:root:lint -- --fix && npm run ci:all:lint -- --fix",
    "man:staged:lint": "lint-staged --concurrent false",
    "___ CI Lint ___": "",
    "ci:space:format": "prettier \"**/*.{js,ts,scss,html,md,json,yml,yaml,*\\w\\wrc}\"",
    "ci:space:styles": "stylelint \"**/*.scss\"",
    "ci:root:lint": "eslint \"*.{js,ts}\" --no-ignore",
    "ci:all:lint": "nx run-many --target lint --all --parallel",
    "___ CI Checks ___": "",
    "ci:affect:test": "nx affected --target test --parallel",
    "ci:affect:build": "nx affected --target build --parallel",
    "ci:all:test": "nx run-many --target test --all --parallel --codeCoverage",
    "ci:all:build": "nx run-many --target build --all --parallel",
    "___ Miscellaneous ___": "",
    "man:dev:update": "keep-updated update --audit-fix",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "prepare": "is-ci || husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@ngrx/component-store": "~13.1.0",
    "@ngrx/effects": "~13.1.0",
    "@ngrx/entity": "~13.1.0",
    "@ngrx/router-store": "~13.1.0",
    "@ngrx/store": "~13.1.0",
    "@nrwl/angular": "13.9.6",
    "@taiga-ui/cdk": "^2.33.0",
    "@taiga-ui/core": "^2.33.0",
    "@taiga-ui/i18n": "^2.33.0",
    "@taiga-ui/icons": "^2.33.0",
    "@taiga-ui/kit": "^2.33.0",
    "@tinkoff/ng-dompurify": "~3.0.0",
    "dompurify": "~2.3.5",
    "jwt-decode": "~3.1.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/builder": "~13.1.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/schematics": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@ngrx/schematics": "~13.1.0",
    "@ngrx/store-devtools": "~13.1.0",
    "@nrwl/cli": "13.9.6",
    "@nrwl/cypress": "13.9.6",
    "@nrwl/eslint-plugin-nx": "13.9.6",
    "@nrwl/jest": "13.9.6",
    "@nrwl/linter": "13.9.6",
    "@nrwl/workspace": "13.9.6",
    "@public-js/eslint-plugin-node": "latest",
    "@public-js/keep-updated": "latest",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "cypress": "^9.1.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "is-ci": "^3.0.0",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "lint-staged": "^12.3.0",
    "nx": "13.9.6",
    "prettier": "^2.5.1",
    "stylelint": "^14.6.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.2"
  },
  "keepUpdated": [
    "@angular-eslint/builder",
    "@angular-eslint/eslint-plugin",
    "@angular-eslint/eslint-plugin-template",
    "@angular-eslint/schematics",
    "@angular-eslint/template-parser",
    "@public-js/eslint-plugin-node",
    "@public-js/keep-updated",
    "@taiga-ui/cdk",
    "@taiga-ui/core",
    "@taiga-ui/i18n",
    "@taiga-ui/icons",
    "@taiga-ui/kit",
    "@types/node",
    "cypress",
    "dompurify",
    "eslint",
    "eslint-config-airbnb-typescript",
    "eslint-config-prettier",
    "eslint-plugin-prettier",
    "husky",
    "is-ci",
    "jwt-decode",
    "lint-staged",
    "prettier",
    "stylelint",
    "stylelint-config-idiomatic-order",
    "stylelint-config-prettier",
    "stylelint-config-standard-scss",
    "stylelint-prettier",
    "tslib",
    "typescript",
    "zone.js"
  ],
  "license": "MIT",
  "author": "Michael M <ghdev@muffin.blue> (https://github.com/mullinsmikey)",
  "homepage": "https://github.com/mullinsmikey/ng-practice-app",
  "lint-staged": {
    "*.{js,ts,scss,html,md,json,yml,yaml,*\\w\\wrc}": "prettier --write",
    "*.{scss}": "stylelint --fix",
    "*.{js,ts}": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^8.0.0"
  }
}
